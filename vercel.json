{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
   "functions": {
     "*.py": {
       "maxDuration": 10,
       "memory": 1024
     }
   },
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}